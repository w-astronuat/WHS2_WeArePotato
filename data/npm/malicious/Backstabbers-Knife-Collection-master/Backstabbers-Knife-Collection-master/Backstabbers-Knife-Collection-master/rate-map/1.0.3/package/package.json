{
	"name": "rate-map",
	"version": "1.0.3",
	"description": "Map a number in the range of 0-1 to a new value with a given range",
	"author": "Shinnosuke Watanabe (https://github.com/shinnn)",
	"repository": "shinnn/rate-map",
	"license": "ISC",
	"scripts": {
		"prebuild": "eslint .",
		"build": "rollup --config=node:module",
